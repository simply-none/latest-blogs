{
  "name": "latest-blogs",
  "version": "6.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/simply-none/latest-blogs.git"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "scripts": {
    "commit": "git pull && node ./time.js && git add . && git-cz",
    "r": "standard-version && git push --follow-tags origin master",
    "prepare": "husky install",
    "init": "npm i docsify-cli -g --registry=https://registry.npmmirror.com",
    "time": "node ./time.js",
    "dev": "node ./time.js & docsify serve"
  },
  "dependencies": {
    "@commitlint/format": "^17.4.4",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@commitlint/cz-commitlint": "^17.5.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-atom": "^3.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "husky": "^8.0.3",
    "inquirer": "^8.2.5",
    "standard-version": "^9.5.0"
  },
  "standard-version": {
    "skip": {
      "changelog": false,
      "tag": false,
      "commit": false
    }
  }
}
